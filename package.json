{
  "name": "valtracker",
  "productName": "VALTracker",
  "version": "0.8.25-alpha",
  "description": "A free Desktop app to look up your VALORANT Statistics and Ingame Collectables.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win",
    "pack": "electron-builder --dir",
    "publish": "electron-builder --win -p always"
  },
  "build": {
    "appId": "VALTracker",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "iconss/VALTracker_Logo_default.ico"
    },
    "nsis": {
      "installerIcon": "iconss/VALTracker_Logo_default.ico",
      "uninstallerIcon": "iconss/VALTracker_Logo_default.ico",
      "uninstallDisplayName": "VALTracker",
      "license": "license.txt",
      "oneClick": false,
      "allowToChangeInstallationDirectory": false
    },
    "publish": [
      {
        "provider": "github",
        "owner": "SpiritLetsPlays",
        "repo": "VALTracker_desktop"
      }
    ]
  },
  "author": "Spirit",
  "repository": "https://github.com/spiritletsplays/VALTracker_desktop",
  "license": "MIT",
  "devDependencies": {
    "electron": "^11.5.0",
    "electron-builder": "^22.14.5",
    "electron-squirrel-startup": "^1.0.0",
    "electron-wix-msi": "^3.2.0"
  },
  "dependencies": {
    "follow-redirects": ">=1.14.8",
    "axios": "^0.25.0",
    "clean-deep": "^3.4.0",
    "discord-rpc": "^4.0.1",
    "ejs": "^3.1.6",
    "ejs-electron": "^2.1.1",
    "electron-fetch": "^1.7.4",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.3",
    "electron-updater": "^4.6.1",
    "fetch": "^1.1.0",
    "fetch-cookie": "^1.0.0",
    "fs-browsers": "^1.1.1",
    "https": "^1.0.0",
    "hubdown": "^2.3.2",
    "jimp": "^0.16.1",
    "jimp-gradient": "^1.0.1",
    "jquery": ">=3.5.0",
    "moment-timezone": "^0.5.34",
    "node-fetch": "^2.6.7",
    "sanitize-filename": "^1.6.3",
    "showdown": "^1.9.0",
    "three-fbx-loader": "^1.0.3",
    "tough-cookie": "^4.0.0",
    "update-electron-app": "^2.0.1",
    "yargs-parser": ">=13.1.2"
  }
}
